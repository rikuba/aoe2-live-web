{
  "functions": {
    "source": "packages/functions",
    "runtime": "nodejs12",
    "predeploy": [
      "yarn workspace @aoe2-live/functions clean",
      "yarn workspace @aoe2-live/functions build"
    ]
  },
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "**",
        "function": "onRequest"
      }
    ]
  }
}
